algoritmo "laço repetiçao"

var
valor1, valor2, resultado : real
cont: inteiro
inicio
cont:= cont +1
   escreval("Digite  o valor 1")
   leia(valor1)
   escreval("Digite o valor 2:")
   leia (valor2)

   resultado:= valor1 + valor2

   escreval("O resultado é:", resultado)
   cont:= cont + 1:
fimenquanto

escreval("Laço repetição finalizado")

fimalgoritmo
