algoritmo "exercicio2"


var
   numero1, numero2:real
inicio
   escreval("Digite um número.")
   leia(numero1)
   escreval("digite outro diferente numero")
   leia (numero2)
   se numero1 > numero2 entao
   escreval (" o numero maior é", numero1)
   senao
   escreval (" o numero maior é", numero2)
   fimse
fimalgoritmo

